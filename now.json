{
  "version": 2,
  "name": "nextjs-express-nowv2",
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/www/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/app/routes/api"
    },
    {
      "src": "/graphql/(.*)",
      "dest": "/app/routes/graphql"
    },
    {
      "src": "/departuretimes/(?<station>[^/]+)$",
      "dest": "/www/departuretimes?station=$station"
    },
    {
      "src": "/trips?fromStation=(?<bookCode>[^/]*)&toStation=(?<chapterNumber>[^/]*)",
      "dest": "/trips?fromStation=$bookCode&toStation=$chapterNumber"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ],
  "builds": [
    {
      "src": "app/routes/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "www/package.json",
      "use": "@now/next"
    }
  ]
}